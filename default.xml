  <project path="gerrit"
           name="tools/gerrit" />

  <project path="gwtjsonrpc"
           name="tools/gwtjsonrpc" />

  <project path="gwtorm"
           name="tools/gwtorm" />

  <!--
      Test environment
  -->
  <project path="test/manifest"
           name="tools/test/manifest" />

  <project path="test/project0"
           name="tools/test/project0" />

  <!--
      External Projects
  -->
