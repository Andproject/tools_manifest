  <project path="gerrit-contactstore"
           name="tools/gerrit-contactstore" />

  <project path="gerrit-keyapplet"
           name="tools/gerrit-keyapplet" />

  <project path="gwtexpui"
           name="tools/gwtexpui" />

  <project path="gwtjsonrpc"
           name="tools/gwtjsonrpc" />

  <project path="gwtorm"
           name="tools/gwtorm" />

  <!--
      Test environment
  -->
  <project path="test/manifest"
           name="tools/test/manifest" />

  <project path="test/project0"
           name="tools/test/project0" />

